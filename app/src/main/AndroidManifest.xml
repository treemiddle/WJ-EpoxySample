<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jay.wj_epoxysample">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.WJEpoxySample">
        <activity
            android:name=".ui.epoxy.SamethingActivity"
            android:exported="false" />
        <activity
            android:name=".ui.epoxy.UpdateActivity"
            android:exported="false" />
        <activity
            android:name=".ui.epoxy.LoadMoreActivity"
            android:exported="false" />
        <activity
            android:name=".ui.epoxy.GridActivity"
            android:exported="false" />
        <activity
            android:name=".ui.epoxy.LinearActivity"
            android:exported="false" />
        <activity
            android:name=".ui.epoxy.EpoxyActivity"
            android:exported="false" />
        <activity
            android:name=".ui.main.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>